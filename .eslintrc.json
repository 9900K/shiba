{
    "extends": "google",
    "env": {
        "node": true
    },
    "rules": {
        "require-jsdoc": 0,
        "generator-star-spacing": 0,
        "curly": [2, "multi", "consistent"],
        "no-multi-spaces": [2, {
            "exceptions": {
                "AssignmentExpression": true,
                "VariableDeclarator": true
            }
        }],
        "quotes": [2, "single", "avoid-escape"],
        "indent": [2, 2, {"SwitchCase": 0}],
        "key-spacing": [2, {"mode": "minimum"}],
        "one-var": 0,
        "no-throw-literal": 0,
        "space-after-keywords": 0,
        "valid-jsdoc": 2,
        "no-extra-parens": 2,
        "no-warning-comments": 0,
        "no-unexpected-multiline": 2,
        "block-scoped-var": 2,
        "complexity": [2, 10],
        "consistent-return": 2,
        "dot-location": [2, "property"],
        "dot-notation": 2,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-caller": 2,
        "no-case-declarations": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-label": 2,
        "no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": 2,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal-escape": 2,
        "no-process-env": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-void": 2,
        "no-with": 2,
        "yoda": 2,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-undefined": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "callback-return": 2,
        "global-require": 2,
        "handle-callback-err": 2,
        "no-mixed-requires": 2,
        "no-new-require": 2,
        "no-path-concat": 2,
        "no-process-exit": 2,
        "no-restricted-modules": 2
    }
}
